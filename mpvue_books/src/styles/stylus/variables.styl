$bgcolor = #00bcd4
$activeColor = #2177c7
$darkText = #ccc
btnStyl()
  width 100%
  height 80rpx
  color white
  background $bgcolor
  margin 10rpx 0
  padding 0 15rpx
  border-radius 5rpx
  font-size 26rpx
  line-height 80rpx
  text-align center
ellipsis()
  overflow hidden
  white-space nowrap
  text-overflow ellipsis
columnCenter()
  display flex
  flex-direction column
  justify-content center
columnTest()
  display flex
  align-items: center;
